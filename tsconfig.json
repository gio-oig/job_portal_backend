{
  "compilerOptions": {
    "outDir": "build",
    "target": "ES2018",
    "module": "commonjs",
    "strict": true,
    "lib": [
      "esnext",
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "build",
    "prisma",
  ],
  "typeRoots": [
    "@types",
    "./node_modules/@types",
  ]
}